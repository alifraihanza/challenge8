<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabel User Game Biodata</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

</head>
<body>
  <a href="/view/createusergamesbiodata" class="btn btn-success m-3">Create User Game Biodata</a>
  <a href="/home" class="btn btn-success m-3">kembali</a>
    <table class="table m-3">
        <thead>
          <tr>
            <th scope="col" class="text-center">No</th>
            <th scope="col" class="text-center">Nama</th>
            <th scope="col" class="text-center">Tanggal Lahir</th>
            <th scope="col" class="text-center">Umur</th>
            <th scope="col" class="text-center">Jenis Kelamin</th>
            <th scope="col" class="text-center">Create at</th>
            <th scope="col" class="text-center">Action</th>
          </tr>
        </thead>
        <tbody>
        <% usergamesbiodata.forEach((usergamebiodata, index) => { %>
          <tr>
            <th scope="row"><%= index+1 %></th>
            <td><%= usergamebiodata.nama %></td>
            <td><%= usergamebiodata.tanggal_lahir %></td>
            <td><%= usergamebiodata.umur %></td>
            <td><%= usergamebiodata.jenis_kelamin %></td>
            <td><%= moment(usergamebiodata.created_at).format('D MMMM, YYYY') %></td>
            <td>
              <a href="/view/updateformusergamesbiodata/<%= usergamebiodata.id %>" class="btn btn-warning">Edit User Game</a>
              <a href="/view/deleteusergamesbiodata/<%= usergamebiodata.id %>" class="btn btn-danger">Hapus</a>
            </td>
          </tr>
        <%})%>
        </tbody>
      </table>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" 
crossorigin="anonymous">
</script>

</html>